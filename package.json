{
  "name": "mac-to-openkey-macro-convert",
  "version": "1.0.0",
  "description": "convert mac keyboard macro in plist format to OpenKey format (colon-separated format)",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-builder --dir -mwl",
    "dist": "electron-builder -mwl",
    "rebuild": "electron-rebuild -f -w ./ --platform=all --out=dist/"
  },
  "build": {
    "appId": "dev.corncob.mac-to-openkey-macro-converter",
    "mac": {
      "category": "util"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrCorncob/mac-to-openkey-macro-convert.git"
  },
  "author": "Corncob",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MrCorncob/mac-to-openkey-macro-convert/issues"
  },
  "homepage": "https://github.com/MrCorncob/mac-to-openkey-macro-convert#readme",
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "bootstrap": "^4.3.1",
    "convert-excel-to-json": "^1.6.1",
    "csv": "^5.1.3",
    "jquery": "^3.4.1",
    "json2csv": "^4.5.4",
    "jsonexport": "^3.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "plist": "^3.0.4",
    "popper.js": "^1.16.0"
  },
  "devDependencies": {
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "electron-rebuild": "^3.2.5"
  }
}
